{
  "author": "Thierry Wong",
  "description": "Tina boilerplate",
  "license": "AGPL-3.0-only",
  "main": "src/exports.ts",
  "module": "src/exports.ts",
  "name": "@gnowth/tina-boilerplate",
  "prettier": "@gnowth/prettier-config",
  "private": true,
  "sideEffects": false,
  "source": true,
  "version": "0.0.0",
  "eslintConfig": {
    "extends": [
      "@gnowth/eslint-config"
    ]
  },
  "jest": {
    "preset": "@gnowth/jest-config"
  },
  "repository": {
    "directory": "packages/boilerplate-tina",
    "type": "git",
    "url": "https://github.com/gnowth/smart.git"
  },
  "dependencies": {
    "tinacms": "1.5.13"
  },
  "devDependencies": {
    "@gnowth/core-logic": "0.0.0",
    "@gnowth/eslint-config": "0.0.0",
    "@gnowth/jest-config": "0.0.0",
    "@gnowth/prettier-config": "0.0.0",
    "@gnowth/typescript-config": "0.0.0",
    "@tinacms/cli": "1.5.22"
  },
  "scripts": {
    "audit": "tinacms audit",
    "build-cms": "tinacms build --local --skip-cloud-checks",
    "clean:modules": "rm -rf node_modules",
    "clean:temp": "rm -rf ./.tina/__generated__ .turbo",
    "lint:script": "eslint --ext ts,mdx .",
    "lint": "npm run lint:script",
    "start-dev": "tinacms dev --noTelemetry --noWatch",
    "typecheck": "tsc"
  }
}
