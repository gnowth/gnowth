{
  "author": "Thierry Wong",
  "description": "Eslint configs",
  "license": "AGPL-3.0-only",
  "main": "hybrid-react.js",
  "module": "hybrid-react.js",
  "name": "@gnowth/eslint-config",
  "prettier": "@gnowth/prettier-config",
  "private": false,
  "sideEffects": false,
  "version": "0.0.0",
  "eslintConfig": {
    "extends": [
      "./javascript"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "directory": "packages/config-eslint",
    "type": "git",
    "url": "https://github.com/gnowth/gnowth.git"
  },
  "dependencies": {
    "@babel/eslint-parser": "7.25.7",
    "@typescript-eslint/eslint-plugin": "8.8.0",
    "@typescript-eslint/parser": "8.8.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.30.0",
    "eslint-plugin-jest": "28.8.3",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-jsx-a11y": "6.10.0",
    "eslint-plugin-mdx": "3.1.5",
    "eslint-plugin-perfectionist": "3.8.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-promise": "7.1.0",
    "eslint-plugin-sonarjs": "2.0.2",
    "eslint-plugin-react": "7.37.1",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-testing-library": "6.3.0"
  },
  "devDependencies": {
    "@gnowth/prettier-config": "0.0.0"
  },
  "peerDependencies": {
    "eslint": "^8.0.0"
  },
  "scripts": {
    "clean:modules": "rm -rf node_modules",
    "clean:temp": "rm -rf .turbo",
    "lint:script": "eslint --ext js .",
    "lint": "npm run lint:script"
  }
}
