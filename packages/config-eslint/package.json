{
  "author": "Thierry Wong",
  "description": "Eslint configs",
  "license": "AGPL-3.0-only",
  "main": "hybrid-react.js",
  "module": "hybrid-react.js",
  "name": "@gnowth/eslint-config",
  "prettier": "@gnowth/prettier-config",
  "private": false,
  "sideEffects": false,
  "version": "0.0.0",
  "eslintConfig": {
    "extends": [
      "./javascript"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "directory": "packages/config-eslint",
    "type": "git",
    "url": "https://github.com/gnowth/gnowth.git"
  },
  "dependencies": {
    "@babel/eslint-parser": "7.22.15",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jest": "27.4.2",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-mdx": "2.2.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-testing-library": "6.0.2"
  },
  "devDependencies": {
    "@gnowth/prettier-config": "0.0.0"
  },
  "peerDependencies": {
    "eslint": "^8.0.0"
  },
  "scripts": {
    "clean:modules": "rm -rf node_modules",
    "clean:temp": "rm -rf .turbo",
    "lint:script": "eslint --ext js .",
    "lint": "npm run lint:script"
  }
}
