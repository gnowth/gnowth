{
  "author": "Thierry Wong",
  "description": "Storybook presets",
  "license": "AGPL-3.0-only",
  "main": "presets/nextjs.ts",
  "module": "presets/nextjs.ts",
  "name": "@gnowth/storybook-preset",
  "prettier": "@gnowth/prettier-config",
  "private": false,
  "sideEffects": false,
  "version": "0.0.0",
  "eslintConfig": {
    "extends": [
      "@gnowth/eslint-config/typescript-react"
    ]
  },
  "exports": {
    ".": {
      "require": "./presets/nextjs.ts",
      "import": "./presets/nextjs.ts",
      "types": "./presets/nextjs.ts"
    },
    "./nextjs": {
      "require": "./presets/nextjs.ts",
      "import": "./presets/nextjs.ts",
      "types": "./presets/nextjs.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "directory": "packages/config-prettier",
    "type": "git",
    "url": "https://github.com/gnowth/gnowth.git"
  },
  "dependencies": {
    "@storybook/addon-essentials": "8.3.2",
    "@storybook/addon-interactions": "8.3.2",
    "@storybook/addon-links": "8.3.2",
    "@storybook/nextjs": "8.3.2",
    "@storybook/preview-api": "8.3.2",
    "storybook": "8.3.2",
    "react": "18.3.1"
  },
  "devDependencies": {
    "@gnowth/babel-preset": "0.0.0",
    "@gnowth/eslint-config": "0.0.0",
    "@gnowth/jest-config": "0.0.0",
    "@gnowth/prettier-config": "0.0.0",
    "@gnowth/typescript-config": "0.0.0"
  },
  "scripts": {
    "clean:modules": "rm -rf node_modules",
    "clean:temp": "rm -rf .turbo",
    "lint:script": "eslint --ext ts,tsx .",
    "lint": "npm run lint:script",
    "typecheck": "tsc"
  }
}
