{
  "author": "Thierry Wong",
  "description": "Storybook presets",
  "license": "AGPL-3.0-only",
  "main": "presets/nextjs.ts",
  "module": "presets/nextjs.ts",
  "name": "@gnowth/storybook-preset",
  "prettier": "@gnowth/prettier-config",
  "private": false,
  "sideEffects": false,
  "version": "0.0.0",
  "eslintConfig": {
    "extends": [
      "@gnowth/eslint-config/typescript-react"
    ]
  },
  "exports": {
    ".": {
      "require": "./presets/nextjs.ts",
      "import": "./presets/nextjs.ts",
      "types": "./presets/nextjs.ts"
    },
    "./nextjs": {
      "require": "./presets/nextjs.ts",
      "import": "./presets/nextjs.ts",
      "types": "./presets/nextjs.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "directory": "packages/config-prettier",
    "type": "git",
    "url": "https://github.com/gnowth/smart.git"
  },
  "?devDependencies": {
    "@storybook/mdx2-csf?": "required for mdx2 feature flag"
  },
  "dependencies": {
    "@chakra-ui/storybook-addon": "4.0.16",
    "@storybook/addon-essentials": "7.0.22",
    "@storybook/addon-interactions": "7.0.22",
    "@storybook/addon-links": "7.0.22",
    "@storybook/builder-webpack5": "7.0.22",
    "@storybook/manager-webpack5": "6.5.16",
    "@storybook/nextjs": "7.0.22",
    "@storybook/preview-api": "7.0.22",
    "storybook": "7.0.22",
    "storybook-react-i18next": "2.0.6",
    "react": "18.2.0",
    "react-query": "3.39.3",
    "next": "13.4.6"
  },
  "devDependencies": {
    "@gnowth/babel-preset": "0.0.0",
    "@gnowth/eslint-config": "0.0.0",
    "@gnowth/jest-config": "0.0.0",
    "@gnowth/prettier-config": "0.0.0",
    "@gnowth/typescript-config": "0.0.0",
    "@storybook/mdx2-csf": "1.1.0"
  },
  "scripts": {
    "clean:modules": "rm -rf node_modules",
    "clean:temp": "rm -rf .turbo",
    "lint:script": "eslint --ext ts,tsx .",
    "lint": "npm run lint:script",
    "typecheck": "tsc"
  }
}
