{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "@gnowth/nextjs-boilerplate#build": {
      "outputs": ["./packages/boilerplate-nextjs/.next", "./artifact/build-web"]
    },
    "@gnowth/nextjs-boilerplate#build-dev": {
      "outputs": ["./packages/boilerplate-nextjs/.next", "./artifact/build-web"]
    },
    "@gnowth/styleguide-boilerplate#build": {
      "dependsOn": ["@gnowth/nextjs-boilerplate#build"],
      "outputs": ["./artifact/build-web/styleguide"]
    },
    "@gnowth/styleguide-boilerplate#build-dev": {
      "dependsOn": ["@gnowth/nextjs-boilerplate#build-dev"],
      "outputs": ["./artifact/build-web/styleguide"]
    },
    "@gnowth/boilerplate-tina#build-cms": {
      "outputs": ["./packages/boilerplate-tina/.tina/__generated__"]
    },
    "build": {
      "outputs": ["./build"]
    },
    "build-dev": {
      "outputs": ["./build"]
    },
    "clean:modules": {
      "cache": false,
      "outputs": []
    },
    "clean:temp": {
      "cache": false,
      "outputs": []
    },
    "lint": {
      "dependsOn": ["@gnowth/boilerplate-tina#build-cms"],
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "test:mock": {
      "outputs": []
    },
    "test:regression": {
      "outputs": []
    },
    "test:smoke": {
      "outputs": []
    },
    "typecheck": {
      "dependsOn": ["@gnowth/boilerplate-tina#build-cms"],
      "outputs": []
    }
  }
}
